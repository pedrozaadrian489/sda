var alertLoaded=true;Vue.component("alert",{name:"alert",template:'<b-alert :show="show" :variant="variant" :v-model="vModel" :dismissible="dismissible" :hide-icon="hideIcon" v-on="$listeners"><div v-show="!hideIcon"><i class="far fa-check-circle icon-success" v-if="variant === \'success\'" ></i><i class="far fe-exclamation-triangle icon-warning" v-if="variant === \'warning\'"></i><i class="far fa-times-circle icon-dismissed" v-if="variant === \'danger\'"></i><i class="fas fa-lightbulb icon-light" v-if="variant === \'secondary\'"></i><i class="fe-info-square d-block" v-if="variant === \'info\'"></i><i class="fe-info-round d-block" v-if="variant === \'info-round\'"></i><i class="fas fa-exclamation-circle d-block" v-if="variant === \'key-file-alert\'"></i></div><p v-html="msg" v-if="msg!=\'\'"></p><slot /><template #dismiss><slot name="dismiss" /></template><div v-if="action" :class="{actionAlert : action}" class="btnLinkAvv linkTableOutBorder" @click="clickAction"><span class="linkText" v-html="msgAction" style="font-size: 12px;"></span><i class="fa fa-angle-right image-action" aria-hidden="true"></i></div></b-alert>',props:{variant:{type:String,},dismissible:{type:String,},msg:{type:String,"default":"",},msgAction:{type:String,"default":"",},action:{type:Boolean,"default":false,},clickAction:{type:Function,"default":function(){return false}},vModel:{type:String,"default":null,},show:{type:String,"default":true,},hideIcon:{type:Boolean,"default":false}},methods:{clickAction:function(){this.$emit("click-action")},}});