Vue.component("navbar",{name:"navbar",props:["language","bankId"],template:'<div><div v-show="fullTimeDay && ipUser" class="float-left position-info-login-ux" id="infoUserDataLogin"><div class="data-login">{{fullTimeDay}}</div><div class="data-login">IP: {{ ipUser}}</div></div><b-navbar id="navbar" toggleable="lg" variant="default"><b-container class="bv-example-row" :class="bankId == \'BAVV\'?\'bavv-logo-container\':\'common-logo-container\'"><b-navbar-brand :href="\'\'==\'true\'?\'/icbs-web/Process?AID=GRAL_VIRTUAL_PAYMENT_LOGIN&RQE=FAC0019C79B6357A&CRFT=fac0019c79b6348e\':\'/icbs-web/AjaxTx?AID=LOGIN&RQE=FAC0019C79B6357B&CRFT=fac0019c79b6348e\'"><img src="images/logo.png" v-if="bankId == \'BOCC\'"/><img src="images/b_pop/logo.png" v-else-if="bankId == \'BPOP\'"/><img src="images/b_avv/red-logo.svg" width="170px" v-else-if="bankId == \'BAVV\'"/><img src="images/b_bta/logo_doc.png" class="extend-logo" v-else-if="bankId == \'BBTA\'"/></b-navbar-brand><b-collapse id="nav-collapse" is-nav><b-navbar-nav class="ml-auto"> <div class="d-flex align-items-center"><a id="link_locale" @click="changeLenguaje" href="/icbs-web/Process?AID=SET_LANGUAGE-0000&RQE=FAC0019C79B6357C&CRFT=fac0019c79b6348e"></a> <input id="language" name="language" type="hidden" value="" /></li><b-button id="langButton" @click="changeLenguaje" variant="link" size="l" v-if="bankId != \'BAVV\'" >{{ language === "en" ? "Espa&ntilde;ol" : "English"}}</b-button></div></b-navbar-nav></b-collapse></b-container></b-navbar></div>',data:function(){return{ipUser:"",fullTimeDay:null}},methods:{changeLenguaje:function(){var d=this.language;if(d==""){$("#language").val("en");d="en"}else{if(d=="en"){$("#language").val("es");d="es"}else{$("#language").val("en");d="en"}}localStorage.setItem("locale",d);var b=$("#link_locale").attr("href")+"&language="+d;var a=getInputs();syncAjaxLoadText({url:b,params:a});if(""=="true"||localStorage.getItem("isPse")){var c="/icbs-web/Process?AID=GRAL_VIRTUAL_PAYMENT_LOGIN-0000&RQE=FAC00