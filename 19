Vue.component("navbar-footer",{props:["id","bankId"],template:'<footer ><b-container><b-row><b-col class="footer-img" ><img class="img-center vigilado-img"/></b-col><b-col class="footer-img"><img class="img-center bocc-img" v-if="bankId == \'BOCC\'" /><img class="img-center bpop-img" v-else-if="bankId == \'BPOP\'"/><img class="img-center bbta-img" v-else-if="bankId == \'BBTA\'"/><img class="img-center bavv-img" v-else-if="bankId == \'BAVV\'"/></b-col><b-col class="footer-img" ><img class="img-center line-img" v-if="bankId != \'BBTA\'"/></b-col><b-col class="footer-img"><img class="img-center verisign-img"   /></b-col><b-col class="footer-img" v-if="bankId == \'BBTA\'"><img class="img-center logo-img" /></b-col></b-row><b-row id="avalRow" ><b-col class="footer-img"><img class="img-center aval-img" /></b-col></b-row></b-container></footer>',computed:{isOccOrPop:function(){return this.bankId=="BOCC"||this.bankId=="BPOP"}}});